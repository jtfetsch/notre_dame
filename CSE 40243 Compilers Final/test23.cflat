 int Fibo(int p,int b)
{
  int sum = 0;
  int p1=p-1;
  int p2=p-2;
  if(p<=0)
    return 0;
  if(p==1)
    return 1; // f0 = 0, f1 = 1
  if(p==2)
    return 1;
  sum = Fibo(p1,b) + Fibo(p2,b);
  return sum;
} 

